@{
    var height = Number(model[0])
}
<table width="100%" height="@height">
<tr>
    <td width="@height" style="background-color: white; text-align: center;">
        <img src="https://picsum.photos/150" class="save" data-type="logo" alt="Random Logo" style="width: @height; height: @height; border-radius: 75%; background-color: white;" />
    </td>
    <td>
        @html.include(model[1].template, model[1].data)
    </td>
</tr>
</table>
